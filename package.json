{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:pwa": "next build && node scripts/generate-pwa-assets.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "pnpm run test:run && pnpm run test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc --noEmit",
    "quality": "pnpm run typecheck && pnpm run lint && pnpm run format:check && pnpm run test:run",
    "prepare": "husky",
    "lighthouse": "lhci autorun",
    "lighthouse:open": "lhci autorun && open ./lighthouse-reports/index.html",
    "security:audit": "pnpm audit --audit-level moderate",
    "bundle:analyze": "pnpm run build:analyze && npx webpack-bundle-analyzer .next/analyze/client.html",
    "sitemap:generate": "node scripts/generate-sitemap.js",
    "tokens:build": "style-dictionary build --config style-dictionary.config.js",
    "prebuild": "pnpm run sitemap:generate",
    "postbuild": "echo 'Build completed successfully!'",
    "prod:setup": "pnpm run sitemap:generate && pnpm run build && pnpm run lighthouse",
    "prod:validate": "pnpm run typecheck && pnpm run lint && pnpm run test:run && pnpm run security:audit"
  },
